  "scripts": {
    "postdeploy": "pg_dump $STAGING_DATABASE_URL | psql $DATABASE_URL"
  },
  "env": {
    "STAGING_DATABASE_URL": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "CLOUDINARY_CLOUD_NAME": {
      "required": true
    },
